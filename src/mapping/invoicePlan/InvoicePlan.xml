<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="moka.invoice.dao.InvoicePlanDao">

    <insert id="insert" parameterType="moka.invoice.bo.InvoicePlan"
        useGeneratedKeys="true" keyProperty="id">
        INSERT INTO t_contract(
        contractId,paymentPlan,paymentPlanFact, versionNo,
        amt,
        planDate, planReceiveDate, receiveAmt,state,
        )
        VALUES (
        #{contractId},#{paymentPlan},#{paymentPlanFact},#{versionNo},#{amt},#{planDate},#{companyIdC},#{planReceiveDate},#{receiveAmt},
        #{state}
        )
    </insert>

    <select id="findCount" resultType="Integer">
        SELECT COUNT(id) FROM
        t_contract;
    </select>

    <select id="findPage" parameterType="moka.basic.page.Page"
        resultType="moka.contract.bo.Contract">
        SELECT * FROM t_contract LIMIT #{pageStart} ,
        #{pageSize};
    </select>

</mapper>